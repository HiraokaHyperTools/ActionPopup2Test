{
  "manifest_version": 2,
  "name": "action_popup",
  "description": "popup tests",
  "version": "0.1",
  "applications": {
    "gecko": {
      "id": "action_popup@digitaldolphins.jp",
      "strict_min_version": "68.6.0"
    }
  },
  "permissions": [],
  "icons": {
    "64": "icons/64.png",
    "48": "icons/48.png",
    "32": "icons/32.png",
    "24": "icons/24.png",
    "16": "icons/16.png"
  },
  "author": "kenjiuno",
  "homepage_url": "https://github.com/HiraokaHyperTools/ActionPopupTest",
  "compose_action": {
    "default_area": "formattoolbar",
    "default_popup": "popup.html",
    "default_title": "action_popup",
    "default_icon": {
      "64": "icons/64.png",
      "48": "icons/48.png",
      "32": "icons/32.png",
      "24": "icons/24.png",
      "16": "icons/16.png"
    }
  },
  "message_display_action": {
    "default_popup": "popup.html",
    "default_title": "action_popup",
    "default_icon": {
      "64": "icons/64.png",
      "48": "icons/48.png",
      "32": "icons/32.png",
      "24": "icons/24.png",
      "16": "icons/16.png"
    }
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "action_popup",
    "default_icon": {
      "64": "icons/64.png",
      "48": "icons/48.png",
      "32": "icons/32.png",
      "24": "icons/24.png",
      "16": "icons/16.png"
    }
  }
}